%create path file


circ = linspace(0,2*pi,50)

position=ones(3,length(circ)*2)

position(:,1:length(circ))=[100*cos(circ);100*sin(circ);zeros(1,length(circ))]
position(:,length(circ)+1:2*length(circ))=[100*cos(circ);zeros(1,length(circ));100*sin(circ)]


color(:,length(circ)+1:2*length(circ))=2

data_struct.s = position;
data_struct.c = color;

save('data')